{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/policy-issuer/src",
  "prefix": "refactoring-smartcore-mf",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/policy-issuer",
        "index": "apps/policy-issuer/src/index.html",
        "main": "apps/policy-issuer/src/main.ts",
        "polyfills": "apps/policy-issuer/src/polyfills.ts",
        "tsConfig": "apps/policy-issuer/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/policy-issuer/src/favicon.ico",
          "apps/policy-issuer/src/assets"
        ],
        "styles": [
          "apps/policy-issuer/src/styles.scss",
          "node_modules/primeicons/primeicons.css",
          "node_modules/primeng/resources/primeng.min.css",
          "node_modules/primeng/resources/themes/lara-light-blue/theme.css"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/policy-issuer/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/policy-issuer/src/environments/environment.ts",
              "with": "apps/policy-issuer/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/policy-issuer/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:webpack-server",
      "configurations": {
        "production": {
          "browserTarget": "policy-issuer:build:production"
        },
        "development": {
          "browserTarget": "policy-issuer:build:development"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4201,
        "publicHost": "http://localhost:4201"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "policy-issuer:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/policy-issuer/**/*.ts",
          "apps/policy-issuer/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/policy-issuer"],
      "options": {
        "jestConfig": "apps/policy-issuer/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "serve-static": {
      "executor": "@nrwl/angular:file-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "policy-issuer:build",
        "port": 4201
      },
      "configurations": {
        "development": {
          "buildTarget": "policy-issuer:build:development"
        },
        "production": {
          "buildTarget": "policy-issuer:build:production"
        }
      }
    }
  },
  "tags": []
}
