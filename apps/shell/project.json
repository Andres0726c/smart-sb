{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/shell/src",
  "prefix": "refactoring-smartcore-mf",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/browser",
        "index": "apps/shell/src/index.html",
        "main": "apps/shell/src/main.ts",
        "polyfills": "apps/shell/src/polyfills.ts",
        "tsConfig": "apps/shell/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          "apps/shell/src/favicon.ico",
          "apps/shell/src/assets",
          {
            "input": "libs/refactoring-smartcore-commons-lib/src/assets",
            "glob": "**/*",
            "output": "smartcore-commons/assets"
          }
        ],
        "styles": [
          "apps/shell/src/styles.scss",
          "node_modules/primeicons/primeicons.css",
          "node_modules/primeng/resources/primeng.min.css",
          "libs/refactoring-smartcore-commons-lib/src/assets/styles/primeng-theme/theme.scss",
          "libs/refactoring-smartcore-commons-lib/src/assets/styles/material-theme/theme.scss"
        ],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/shell/webpack.config.js"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "libs/refactoring-smartcore-commons-lib/src/lib/shared/environments/environment.ts",
              "with": "libs/refactoring-smartcore-commons-lib/src/lib/shared/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/shell/webpack.prod.config.js"
          }
        },
        "stage": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "libs/refactoring-smartcore-commons-lib/src/lib/shared/environments/environment.ts",
              "with": "libs/refactoring-smartcore-commons-lib/src/lib/shared/environments/environment.stage.ts"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/shell/webpack.prod.config.js"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true,
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/shell/webpack.prod.config.js"
          }
        },
        "local": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nrwl/angular:module-federation-dev-server",
      "configurations": {
        "production": {
          "browserTarget": "shell:build:production"
        },
        "stage": {
          "browserTarget": "shell:build:stage"
        },
        "development": {
          "browserTarget": "shell:build:development"
        },
        "local": {
          "browserTarget": "shell:build:local"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "port": 4200,
        "publicHost": "http://localhost:4200"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "shell:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/shell/**/*.ts", "apps/shell/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/shell"],
      "options": {
        "jestConfig": "apps/shell/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["policy-management"]
}
