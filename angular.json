{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"shell": {
			"projectType": "application",
			"schematics": {
				"@schematics/angular:component": {
					"style": "scss"
				}
			},
			"root": "projects/shell",
			"sourceRoot": "projects/shell/src",
			"prefix": "app",
			"architect": {
				"build": {
					"builder": "ngx-build-plus:browser",
					"options": {
						"outputPath": "dist/browser",
						"index": "projects/shell/src/index.html",
						"main": "projects/shell/src/main.ts",
						"polyfills": "projects/shell/src/polyfills.ts",
						"tsConfig": "projects/shell/tsconfig.app.json",
						"inlineStyleLanguage": "scss",
						"assets": [
							"projects/shell/src/favicon.ico",
							"projects/shell/src/assets",
							{
								"input": "libs/commons-lib/src/lib/assets",
								"glob": "**/*",
								"output": "smartcore-commons/assets"
							}
						],
						"styles": [
							"projects/shell/src/styles.scss",
							"node_modules/primeicons/primeicons.css",
							"node_modules/primeng/resources/primeng.min.css",
							"libs/commons-lib/src/lib/assets/styles/primeng-theme/theme.scss",
							"libs/commons-lib/src/lib/assets/styles/material-theme/theme.scss"
						],
						"scripts": [],
						"allowedCommonJsDependencies": [
							"uuid",
							"lodash",
							"url",
							"buffer",
							"ulid",
							"camelcase-keys",
							"@aws-crypto",
							"@aws-crypto/sha256-js",
							"@aws-crypto/sha256-browser",
							"@aws-crypto/crc32",
							"fast-xml-parser",
							"crypto-js",
							"isomorphic-unfetch",
							"paho-mqtt",
							"js-cookie",
							"@aws-amplify/ui",
							"cookie",
							"events"
						],
						"extraWebpackConfig": "projects/shell/webpack.config.js",
						"commonChunk": false
					},
					"configurations": {
						"production": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "6mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2mb",
									"maximumError": "6mb"
								}
							],
							"fileReplacements": [
								{
									"replace": "libs/commons-lib/src/lib/environments/environment.ts",
									"with": "libs/commons-lib/src/lib/environments/environment.prod.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/shell/webpack.prod.config.js"
						},
						"stage": {
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "6mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "2mb",
									"maximumError": "6mb"
								}
							],
							"fileReplacements": [
								{
									"replace": "libs/commons-lib/src/lib/environments/environment.ts",
									"with": "libs/commons-lib/src/lib/environments/environment.stage.ts"
								}
							],
							"outputHashing": "all",
							"extraWebpackConfig": "projects/shell/webpack.prod.config.js"
						},
						"development": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true,
							"fileReplacements": [
								{
									"replace": "libs/commons-lib/src/lib/environments/environment.ts",
									"with": "libs/commons-lib/src/lib/environments/environment.dev.ts"
								}
							],
							"outputHashing": "all"
						},
						"local": {
							"buildOptimizer": false,
							"optimization": false,
							"vendorChunk": true,
							"extractLicenses": false,
							"sourceMap": true,
							"namedChunks": true
						}
					},
					"defaultConfiguration": "production"
				},
				"serve": {
					"builder": "ngx-build-plus:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "shell:build:production",
							"extraWebpackConfig": "projects/shell/webpack.prod.config.js"
						},
						"stage": {
							"browserTarget": "shell:build:stage",
							"extraWebpackConfig": "projects/shell/webpack.prod.config.js"
						},
						"development": {
							"browserTarget": "shell:build:development"
						},
						"local": {
							"browserTarget": "shell:build:local"
						}
					},
					"defaultConfiguration": "local",
					"options": {
						"port": 4200,
						"publicHost": "http://localhost:4200",
						"extraWebpackConfig": "projects/shell/webpack.config.js"
					}
				},
				"extract-i18n": {
					"builder": "ngx-build-plus:extract-i18n",
					"options": {
						"browserTarget": "shell:build",
						"extraWebpackConfig": "projects/shell/webpack.config.js"
					}
				},
				"test": {
					"builder": "@angular-builders/jest:run",
					"options": {
						"configPath": "./jest.config.js"
					}
				}
			}
		},
