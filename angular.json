{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shell": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/shell",
      "sourceRoot": "projects/shell/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/shell",
            "index": "projects/shell/src/index.html",
            "main": "projects/shell/src/main.ts",
            "polyfills": "projects/shell/src/polyfills.ts",
            "tsConfig": "projects/shell/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/shell/src/favicon.ico",
              "projects/shell/src/assets",
              {
                "input": "projects/commons-lib/src/lib/assets",
                "glob": "**/*",
                "output": "smartcore-commons/assets"
              }
            ],
            "styles": [
              "projects/shell/src/styles.scss",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css",
              "projects/commons-lib/src/lib/assets/styles/primeng-theme/theme.scss",
              "projects/commons-lib/src/lib/assets/styles/material-theme/theme.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "uuid",
              "lodash",
              "url",
              "buffer",
              "ulid",
              "camelcase-keys",
              "@aws-crypto",
              "@aws-crypto/sha256-js",
              "@aws-crypto/sha256-browser",
              "@aws-crypto/crc32",
              "fast-xml-parser",
              "crypto-js",
              "isomorphic-unfetch",
              "paho-mqtt",
              "js-cookie",
              "@aws-amplify/ui",
              "cookie"
            ],
            "extraWebpackConfig": "projects/shell/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/shell/src/environments/environment.ts",
                  "with": "projects/shell/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "projects/shell/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "shell:build:production",
              "extraWebpackConfig": "projects/shell/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "shell:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4200,
            "publicHost": "http://localhost:4200",
            "extraWebpackConfig": "projects/shell/webpack.config.js"
          }
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "shell:build",
            "extraWebpackConfig": "projects/shell/webpack.config.js"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "configPath": "./jest.config.js"
          }
        }
      }
    },
    "auth": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/auth",
      "sourceRoot": "projects/auth/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist/auth",
            "index": "projects/auth/src/index.html",
            "main": "projects/auth/src/main.ts",
            "polyfills": "projects/auth/src/polyfills.ts",
            "tsConfig": "projects/auth/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/auth/src/favicon.ico",
              "projects/auth/src/assets"
            ],
            "styles": [
              "projects/auth/src/styles.scss"
            ],
            "scripts": [],
            "extraWebpackConfig": "projects/auth/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/auth/src/environments/environment.ts",
                  "with": "projects/auth/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "projects/auth/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "auth:build:production",
              "extraWebpackConfig": "projects/auth/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "auth:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4201,
            "publicHost": "http://localhost:4201",
            "extraWebpackConfig": "projects/auth/webpack.config.js"
          }
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "auth:build",
            "extraWebpackConfig": "projects/auth/webpack.config.js"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "configPath": "./jest.config.js"
          }
        }
      }
    },
    "commons-lib": {
      "projectType": "library",
      "root": "projects/commons-lib",
      "sourceRoot": "projects/commons-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/commons-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/commons-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/commons-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "tsConfig": "./jest.config.js"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}